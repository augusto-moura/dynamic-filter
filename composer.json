{
	"name": "augusto-moura/dynamic-filter",
	"description": "A Laravel library for rendering dynamic filters that provide input data for API calls.",
	"type": "library",
	"require": {
		"php": ">= 7.1.3",
		"laravel/framework": "^6.0|^7.0|^8.0"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Augusto Moura",
			"email": "augusto_moura40@hotmail.com"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"AugustoMoura\\DynamicFilter\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"AugustoMoura\\DynamicFilter\\DynamicFilterServiceProvider"
			]
		}
	},
	"require-dev": {
		"symfony/var-dumper": "^4.3",
		"vlucas/phpdotenv": "^3.6",
		"orchestra/testbench-dusk": "^4.3",
		"orchestra/testbench": "^4.0"
	}
}
